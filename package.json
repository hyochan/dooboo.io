{
  "name": "dooboo.io",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dooboo.io",
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss --input ./styles/root.css --output ./styles/output.css --watch\"",
    "build": "tailwindcss ./styles/root.css --output ./styles/output.css && yarn generate:prisma && next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "seed": "dotenv -e .env.local -- prisma db seed",
    "migrate:dev": "dotenv -e .env.dev -- prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "generate:prisma": "prisma generate",
    "generate:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_DOOBOOIO --schema public > src/types/supabase.ts",
    "generate": "yarn generate:prisma && yarn generate:supabase"
  },
  "dependencies": {
    "@crello/react-lottie": "^0.0.11",
    "@formatjs/intl-localematcher": "^0.2.32",
    "@next/font": "v13.1.7-canary.6",
    "@primer/octicons-react": "^17.12.0",
    "@prisma/client": "4.10.1",
    "@supabase/supabase-js": "^2.8.0",
    "axios": "^1.3.3",
    "client-only": "^0.0.1",
    "negotiator": "^0.6.3",
    "next": "^13.1.6",
    "prisma": "^4.10.1",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.1",
    "react-simple-snackbar": "^1.1.11",
    "react-text-transition": "^3.0.2",
    "server-only": "^0.0.1",
    "tiny-invariant": "^1.3.1",
    "typescript": "4.9.5",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@dooboo/eslint-config-react": "^1.0.4",
    "@tailwindcss/forms": "^0.5.3",
    "@types/negotiator": "^0.6.1",
    "@types/node": "18.14.0",
    "@types/react": "18.0.28",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "13.1.6",
    "flatten-tailwindcss-theme": "^1.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "supabase": ">=1.8.1",
    "tailwind-merge": "^1.9.1",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./prisma/seed.ts"
  },
  "packageManager": "yarn@3.4.1"
}
